srcdir = @srcdir@
top_srcdir = @top_srcdir@
top_builddir = @top_builddir@

SOURCES =\
	lbd.c \
	command.c \
	lbdcmdline.c \

TARGETS =\
	lbd

include $(top_builddir)/make.build

lbd: $(OBJECTS) $(top_srcdir)/src/lib/libjsraid.a
	$(Q) $(CC) $(CFLAGS) $(LDFLAGS) $(ELDFLAGS) -o $@ $^

jsraid: all

all: FORCE

.PHONY: FORCE jsraid
FORCE:
